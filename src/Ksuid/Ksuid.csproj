<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>K-Sortable Globally Unique IDs</Description>
    <TargetFrameworks>net462;netstandard2.0</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <Version>0.9.0</Version>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>base62</PackageTags>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Chino Chang</Authors>
    <PackageId>Ksuid</PackageId>
    <PackageVersion>0.9.0</PackageVersion>
    <PackageProjectUrl>https://github.com/JoyMoe/Ksuid.Net</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/JoyMoe/Ksuid.Net/blob/master/LICENSE</PackageLicenseUrl>
    <Title>Ksuid</Title>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net462'">
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Base62-Net" Version="1.1.0-build6" />
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" />
    <PackageReference Include="System.Runtime.Extensions" Version="4.3.0" />
  </ItemGroup>
</Project>